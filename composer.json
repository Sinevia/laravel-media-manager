{
    "name": "sinevia/laravel-media-manager",
    "description": "Media manager for Laravel - browse / create / rename / delete files and folders.",
    "type": "library",
    "keywords": ["media", "manager", "laravel", "php", "Sinevia"],    
    "homepage": "https://github.com/Sinevia/laravel-media-manager",
    "license": "commercial",
    "authors": [
        {
            "name": "Milan Lesichkov",
            "email": "lesichkovm@gmail.com",
            "homepage": "http://lesichkov.co.uk",
            "role": "Lead Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sinevia\\LaravelMediaManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sinevia\\LaravelMediaManager\\": "tests/"
        }
    },
     "require": {
        "laravel/framework": "5.*"
    },
    "require-dev": {        
        "orchestra/testbench": "^3.5@dev",
        "phpunit/phpunit": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sinevia\\LaravelMediaManager\\LaravelMediaManagerServiceProvider"
             ]
        }
    }
}
